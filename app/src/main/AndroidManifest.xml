<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="id.kharisma.studio.catelink">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CateLink"
        tools:targetApi="31">
        <activity
            android:name=".Activity.SettingsActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.FavoriteActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.RecommendActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.Home_catemenuActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.User_Profile_Activity"
            android:exported="false" />
        <activity
            android:name=".Activity.Edit_Profile_Activity"
            android:exported="false" />
        <activity
            android:name=".Activity.PesananActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.Detailmenu_Activity"
            android:exported="false" />
        <activity
            android:name=".Activity.KeranjangActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.CustomListView"
            android:exported="false" />
        <activity
            android:name=".Activity.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name=".Activity.SignupActivity"
            android:exported="true"
            android:parentActivityName=".Activity.MainActivity" />
        <activity
            android:name=".Activity.LoginActivity"
            android:exported="true"
            android:parentActivityName=".Activity.MainActivity" />
        <activity
            android:name=".Activity.SplashscnActivity"
            android:exported="true"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>